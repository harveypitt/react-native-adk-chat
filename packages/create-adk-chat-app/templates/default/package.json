{
  "name": "adk-chat-app",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "concurrently --kill-others --names \"PROXY,APP\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run proxy\" \"expo start\"",
    "android": "concurrently --kill-others --names \"PROXY,APP\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run proxy\" \"expo start --android\"",
    "ios": "concurrently --kill-others --names \"PROXY,APP\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run proxy\" \"expo start --ios\"",
    "web": "concurrently --kill-others --names \"PROXY,APP\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run proxy\" \"expo start --web\"",
    "proxy": "echo 'No proxy server configured, skipping...'"
  },
  "dependencies": {
    "@react-native-adk-chat/client": "github:harveypitt/react-native-adk-chat#main:packages/client",
    "@expo/metro-runtime": "~4.0.1",
    "expo": "~52.0.0",
    "expo-asset": "~11.0.1",
    "expo-constants": "~17.0.3",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.0",
    "react-native-web": "~0.19.13",
    "react-native-safe-area-context": "^4.12.0",
    "@expo/vector-icons": "^14.0.0",
    "react-native-dotenv": "^3.4.11",
    "@react-navigation/native": "^7.1.24",
    "@react-navigation/stack": "^7.6.11",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-screens": "^4.18.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "concurrently": "^8.2.2",
    "@types/react": "~18.3.12",
    "typescript": "~5.3.3"
  }
}
